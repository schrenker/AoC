func TestDayDDPartOne(t *testing.T) {
	d := yYYYY.DayDD{}
	os.Args = []string{"cmd", "YYYY", "DD", "01"}
	tools.GetInput("YYYY", "DD")
    t.Errorf("No tests defined. Placeholder: %v", d)
}

